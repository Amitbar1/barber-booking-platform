{
  "version": 2,
  "buildCommand": "npm ci && npm run build",
  "outputDirectory": "apps/web/dist",
  "installCommand": "npm ci",
  "builds": [
    {
      "src": "apps/web/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "apps/web/dist/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
